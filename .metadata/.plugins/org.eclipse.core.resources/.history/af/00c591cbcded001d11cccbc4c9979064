package app;

public class Main {
	 public static void main(String[] args) {
	     GoldenRetriever dog = new GoldenRetriever("Buddy", 3, "ball");
	     dog.displayInfo(); // Inherited from Animal
	     dog.displayBreed(); // Inherited from Dog
	     dog.makeSound(); // Overridden method from Animal
	     dog.play(); // Method specific to GoldenRetriever
	 }
	}